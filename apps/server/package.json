    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "prisma": "^4.15.0",
    "regenerator-runtime": "^0.13.11",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "typescript": "5.1.3"
  },
  "dependencies": {
