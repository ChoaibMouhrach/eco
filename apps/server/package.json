{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "scripts": {
    "dev": "nodemon -r tsconfig-paths/register src/server.ts",
    "build": "rm -rf dist && swc src -d dist --copy-files",
    "start": "node dist/server.js",
    "lint": "eslint \"**/*.{ts,tsx}\"",
    "lint:fix": "eslint --fix \"**/*.{ts,tsx}\"",
    "format": "prettier --write .",
    "migrate": "prisma migrate dev",
    "test": "jest",
    "seed": "prisma db seed"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.62",
    "@swc/helpers": "^0.5.1",
    "@swc/jest": "^0.2.26",
    "@types/cookie-parser": "^1.4.3",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^29.5.2",
    "@types/morgan": "^1.9.4",
    "@types/node": "20.2.5",
    "@types/nodemailer": "^6.4.8",
    "@types/supertest": "^2.0.12",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "prisma": "^4.15.0",
    "regenerator-runtime": "^0.13.11",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "typescript": "5.1.3"
  },
  "dependencies": {
    "@prisma/client": "4.15.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.1.4",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.3",
    "tsconfig-paths": "^4.2.0",
    "zod": "^3.21.4"
  }
}
